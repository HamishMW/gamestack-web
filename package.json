    "deploy-prod": "firebase use prod && firebase deploy --only hosting",
    "serve": "firebase serve --only hosting",
    "eject": "react-scripts eject"
  },
  "reactSnap": {
    "include": [
      "/privacy",
      "/support"
    ],
    "crawl": true,
    "skipThirdPartyRequests": true,
